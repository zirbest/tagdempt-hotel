{
  "name": "zirtech-pos",
  "version": "1.0.0",
  "description": "An Electron application with Solid and TypeScript",
  "author": "example.com",
  "homepage": "https://www.electronjs.org",
  "main": "./out/main/index.js",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev -w",
    "debug": "electron-vite dev --inspect --sourcemap",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "electron-vite build && electron-builder --mac --config",
    "build:linux": "electron-vite build && electron-builder --linux --config",
    "db:push": "drizzle-kit push:sqlite",
    "db:studio": "drizzle-kit studio",
    "db:gen": "drizzle-kit generate:sqlite",
    "db:seed": "npx tsx src/main/db/seed.ts"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^2.0.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "better-sqlite3": "^9.2.2",
    "drizzle-orm": "^0.29.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.4.3",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.3",
    "@faker-js/faker": "^8.3.1",
    "@iconify-json/mingcute": "^1.1.13",
    "@kobalte/core": "^0.11.2",
    "@solidjs/router": "^0.9.1",
    "@types/better-sqlite3": "^7.6.8",
    "@types/node": "^18.19.3",
    "autoprefixer": "^10.4.16",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "drizzle-kit": "^0.20.6",
    "electron": "^22.3.27",
    "electron-builder": "^24.6.3",
    "electron-vite": "^1.0.27",
    "eslint": "^8.55.0",
    "eslint-plugin-solid": "^0.13.0",
    "postcss": "^8.4.32",
    "solid-js": "^1.8.7",
    "tailwind-merge": "^2.1.0",
    "tailwindcss": "^3.3.6",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.3.3",
    "unplugin-icons": "^0.18.1",
    "vite": "^4.4.9",
    "vite-plugin-solid": "^2.8.0"
  }
}
