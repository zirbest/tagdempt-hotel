{
  "name": "zirtech-tagdempt-hotel",
  "version": "1.0.2-beta.3",
  "description": "An Electron application with Solid and TypeScript",
  "author": "example.com",
  "homepage": "https://www.electronjs.org",
  "main": "./out/main/index.js",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev -w",
    "debug": "electron-vite dev --inspect --sourcemap",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "electron-vite build && electron-builder --mac --config",
    "build:linux": "electron-vite build && electron-builder --linux --config",
    "db:push": "drizzle-kit push:sqlite",
    "db:studio": "drizzle-kit studio",
    "db:gen": "drizzle-kit generate:sqlite",
    "db:seed": "npx tsx src/main/db/seed.ts"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "@libsql/client": "^0.4.3",
    "@modular-forms/solid": "^0.20.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "drizzle-orm": "^0.29.3",
    "drizzle-valibot": "^0.1.1",
    "libsql": "^0.2.0",
    "valibot": "^0.27.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.3",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^2.2.1",
    "@faker-js/faker": "^8.4.0",
    "@iconify-json/mingcute": "^1.1.15",
    "@kobalte/core": "^0.12.1",
    "@solidjs/router": "^0.10.10",
    "@types/better-sqlite3": "^7.6.9",
    "@types/node": "^20.11.9",
    "autoprefixer": "^10.4.17",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.3.1",
    "drizzle-kit": "^0.20.13",
    "electron": "^22.3.27",
    "electron-builder": "^24.9.1",
    "electron-vite": "^2.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-solid": "^0.13.1",
    "postcss": "^8.4.33",
    "solid-js": "^1.8.12",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.3.3",
    "unplugin-icons": "^0.18.3",
    "vite": "^4.5.2",
    "vite-plugin-solid": "^2.9.1"
  }
}
